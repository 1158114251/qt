<Rect color="blue" width="800" height="600" id="Page">
    <ListModel id="DataSource">
        <Month>
            <name>January</name>
        </Month>
        <Month>
            <name>February</name>
        </Month>
    </ListModel>
    <HorizontalLayout>
        <Repeater dataSource="{DataSource}">
            <component>
                <Rect color="red" width="100" height="100">
                    <Text text="{name}" />
                </Rect>
            </component>
        </Repeater>
    </HorizontalLayout>
</Rect>
