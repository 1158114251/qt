<Rect color="blue" width="800" height="600" id="Page">
    <HorizontalLayout>
        <Repeater >
            <component>
                <Rect color="red" width="100" height="100">
                    <Text text="{name}" />
                </Rect>
            </component>
            <dataSource>
                <ListModel>
                    <Month>
                        <name>January</name>
                    </Month>
                    <Month>
                        <name>February</name>
                    </Month>
                </ListModel>
            </dataSource>
        </Repeater>
    </HorizontalLayout>
</Rect>
