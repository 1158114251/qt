<Rect color="blue" width="800" height="600" id="Page">
    <ListView anchors.fill="{parent}">
        <delegate>
            <Rect color="red" width="100" height="100">
                <Text text="{name}" />
            </Rect>
        </delegate>
        <model>
            <ListModel>
                <Month>
                    <name>January</name>
                </Month>
                <Month>
                    <name>February</name>
                </Month>
            </ListModel>
        </model>
    </ListView>
</Rect>
