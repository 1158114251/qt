<Rect color="blue" width="800" height="600" id="Page">
    <ListModel id="Model">
        <Month>
            <name>January</name>
        </Month>
        <Month>
            <name>February</name>
        </Month>
    </ListModel>

    <ListView anchors.fill="{parent}" model="{Model}">
        <delegate>
            <Rect color="red" width="100" height="100">
                <Text text="{name}" />
            </Rect>
        </delegate>
    </ListView>
</Rect>
