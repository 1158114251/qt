          <property name="text">
            <string>&amp;New Connection</string>
          </property>
        </widget>
      </item>
      <item row="0" column="0" rowspan="5" colspan="1">
        <widget class="Q3ListBox" name="listConnections"/>
      </item>
      <item row="1" column="1">
        <widget class="QPushButton" name="buttonDelete">
          <property name="text">
            <string>&amp;Delete Connection</string>
          </property>
        </widget>
