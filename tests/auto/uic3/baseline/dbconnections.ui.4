              <property name="enabled">
                <bool>false</bool>
              </property>
              <property name="text">
                <string>Connec&amp;t</string>
              </property>
            </widget>
          </item>
          <item>
            <spacer name="Spacer5">
              <property name="sizeHint">
                <size>
                  <width>20</width>
                  <height>20</height>
